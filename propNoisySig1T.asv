N= size(X,2);

if (size(W,1)~=size(W,2))
    disp('dimension Mismatch'); 
end
if (size(W,1)~=length(s))
    disp('dimension Mismatch');
end


g = @nonLin;
%y = s;


%Compute the values of the network after propagating one time step

for lay=N:2
   Y(:,N) = g(W(:,:,lay-1)*X(:,N-1))
    
end

for i=p:q-1
    temp = g(W(:,:,i)*y);
    y=temp;
end


end

